@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>@ViewBag.Title</title>

    <!-- CSS extended from Bootstrap -->
    <link href="../../Content/CSS/Extended/Main.css" rel="stylesheet">
    <!-- Bootstrap core CSS -->
    <link href="../../Content/CSS/Bootstrap/css/bootstrap.css" rel="stylesheet">
</head>

<body>
    @Html.MvcSiteMap().Menu("BootstrapNavBarHelperModel")
    <div>
        @RenderBody()
    </div>
    <hr />
    <div class="container">
        <footer>
            <p class="pull-right">
                <a href="#">Back to top</a>
            </p>
            <p>&copy; 2013 Company, Inc. &middot; <a href="#">Privacy</a> &middot; <a href="#">Terms</a></p>
        </footer>
    </div>

    <!-- Scripts at end of document so page loads quicker -->
    <script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
    <script src="../../Content/CSS/Bootstrap/js/bootstrap.min.js"></script>
</body>
</html>
